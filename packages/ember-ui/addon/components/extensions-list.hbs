<div role="menu" class="next-catalog-menu-items flex" ...attributes>
    {{#each @extensions as |extension|}}
        <LinkToExternal @route={{concat "console." (dasherize extension.extension)}} class={{@itemClass}} role="menuitem">
            <div class="w-6">
                <FaIcon @icon={{extension.icon}} @size="sm" />
            </div>
            <div>
                <span>{{extension.extension}}</span>
            </div>
        </LinkToExternal>
    {{/each}}
